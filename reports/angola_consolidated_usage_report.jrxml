	<parameter name="startDate" class="java.lang.String"/>
	<parameter name="endDate" class="java.lang.String"/>
	<parameter name="imagesDirectory" class="java.lang.String" isForPrompting="false"/>
	<parameter name="timeZone" class="java.lang.String" isForPrompting="false"/>
	<queryString language="plsql">
		<![CDATA[select 
	facilities.facilityName as facilityName,
	issuesAndReceipts.eventCount as issuesAndReceiptsCount,
	adjustments.eventCount as adjustmentsCount,
	inventory.eventCount as inventoryCount,
	requisitions.eventCount as requisitionsCount
from
(select
		f."name" as facilityName 
		from referencedata.facilities f) as facilities
left join 
(select
		count(distinct se.id) as eventCount,
		f."name" as facilityName
	from stockmanagement.stock_events se
	join referencedata.facilities f on f.id = se.facilityid
	left join stockmanagement.stock_event_line_items seli on seli.stockeventid = se.id
	where se.processeddate::varchar >= $P{startDate}::varchar
					and se.processeddate::varchar <= ($P{endDate}::timestamp + interval '1 day')::varchar
	and se.id not in (
		select pi.stockeventid from stockmanagement.physical_inventories pi
		where pi.isdraft = false
	)
	and (seli.destinationid is not null or seli.sourceid is not null)
	group by facilityName) as issuesAndReceipts
	on issuesAndReceipts.facilityName = facilities.facilityName
left join
	(select
		count(distinct se.id) as eventCount,
		f."name" as facilityName
	from stockmanagement.stock_events se
	join referencedata.facilities f on f.id = se.facilityid
	left join stockmanagement.stock_event_line_items seli on seli.stockeventid = se.id
	where se.processeddate::varchar >= $P{startDate}::varchar
					and se.processeddate::varchar <= ($P{endDate}::timestamp + interval '1 day')::varchar
	and se.id not in (
		select pi.stockeventid from stockmanagement.physical_inventories pi
		where pi.isdraft = false
	)
	and seli.destinationid is null and seli.sourceid is null
	group by facilityName) as adjustments
	on adjustments.facilityName = facilities.facilityName
left join (select
				count(se.id) as eventCount,
				f."name" as facilityName
				from stockmanagement.stock_events se
				join referencedata.facilities f on f.id = se.facilityid
				where se.processeddate::varchar >= $P{startDate}::varchar
					and se.processeddate::varchar <= ($P{endDate}::timestamp + interval '1 day')::varchar
					and se.id in (
						select pi.stockeventid from stockmanagement.physical_inventories pi
						where pi.isdraft = false
					)
			group by facilityName) as inventory
			on inventory.facilityName = facilities.facilityName
left join (select 
count(distinct r.id) as eventCount,
f."name" as facilityName
from requisition.requisitions r 
join referencedata.facilities f on f.id = r.facilityid
where r.createddate::varchar >= $P{startDate}::varchar
					and r.createddate::varchar <= ($P{endDate}::timestamp + interval '1 day')::varchar
					and r.status not in ('SKIPPED')
group by facilityName) as requisitions
on requisitions.facilityName = facilities.facilityName
			order by facilityName]]>
	</queryString>
