	<queryString language="plsql">
		<![CDATA[WITH RECURSIVE findChildZones AS (
  SELECT * FROM referencedata.geographic_zones WHERE name = $P{district}::varchar
  UNION ALL
  SELECT referencedata.geographic_zones.* FROM referencedata.geographic_zones JOIN findChildZones ON findChildZones.id = referencedata.geographic_zones.parentId)

SELECT
	products.productName AS productname,
	CONCAT_WS(': ',facilities.code,facilities.name) AS origin,
	organizations.name AS destination,
	SUM(line_items.quantity) AS quantity
FROM
	stockmanagement.stock_event_line_items AS line_items
	JOIN stockmanagement.stock_events AS stock_events ON stock_events.id = line_items.stockeventid
	JOIN stockmanagement.nodes AS nodes ON nodes.id = line_items.destinationid
	JOIN stockmanagement.organizations AS organizations ON organizations.id = nodes.referenceid
	JOIN referencedata.programs AS programs ON programs.id = stock_events.programid
	JOIN referencedata.facilities AS facilities ON facilities.id = stock_events.facilityid
	JOIN referencedata.geographic_zones AS zones ON zones.id = facilities.geographiczoneid
	JOIN (
	    select
	        id,
	        code as productCode,
	        fullproductname as productName,
	        RANK () OVER (
	            PARTITION BY id
	            ORDER BY versionnumber DESC
	        ) AS productVersionRank
	    from referencedata.orderables
	) AS products ON products.id = line_items.orderableid
WHERE productVersionRank = 1
	AND ($P{program} IS NULL OR $X{IN, programs.name, programList})
	AND ($P{district}::varchar IS NULL OR zones.id IN (SELECT id FROM findChildZones))
	AND ($P{productId} IS NULL OR products.id = $P{productId}::uuid)
	AND ($P{occurreddate} IS NULL OR line_items.occurreddate = $P{occurreddateParsed}::date)
GROUP BY productname, origin, destination
ORDER BY origin, productname]]>
	</queryString>
